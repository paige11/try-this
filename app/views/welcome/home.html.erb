<div class="container-fluid">
  <h1 class="turquoise">Welcome, <%= @user_greeting %></h1>

  <h3>Here are the most recent questions from users:</h3>

  <% @questions.each do |question| %>
    <p><%= link_to question.content, question_path(question) %></p>
  <% end %>

  <h3>Browse by category:</h3>
  <% @categories.each do |category| %>
    <li><%= link_to category.name, category_path(category) %></li>
  <% end %>
</div>
